---
import { Image } from 'astro:assets';
import logo from '../../assets/GP_logo.png';
interface ImageMetadata {
  src: string;
  width: number;
  height: number;
  format: string;
}
---

<Image
  class="logo"
  src={logo}
  alt="Logo Guiadeparche"
/>

<style>
  .logo {
    position: fixed;
    right: 0;
    bottom: 0;
    width: 100px;
    height: 100px;
  }
</style>
